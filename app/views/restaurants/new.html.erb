<div class="container">
  <h1 class="fw-bold">New restaurant listing</h1>
  <hr class="border border-danger">
  <div class=" container mt-2 p-2 rounded bg-dark text-light align-middle fs-3">
    <%= simple_form_for(@restaurant) do |f| %>
      <%= f.input :name %>
      <%= f.input :category %>
      <%= f.input :address %>
      <%= f.input :phone_number %>
      <%= f.submit %>
    <% end %>
  </div>
</div>
